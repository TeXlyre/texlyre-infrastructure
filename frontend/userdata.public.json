{
  "settings": {
    "editor-font-size": "base",
    "editor-font-family": "monospace",
    "editor-show-line-numbers": true,
    "editor-syntax-highlighting": true,
    "editor-theme-highlights": "auto",
    "editor-spell-check": false,
    "editor-auto-save-enable": true,
    "editor-auto-save-delay": 1000,
    "file-system-backup-enable": false,
    "file-system-backup-auto-backup": false,
    "bibtex-viewer-auto-tidy": false,
    "bibtex-viewer-tidy-options": "standard",
    "image-viewer-auto-center": true,
    "image-viewer-quality": "high",
    "image-viewer-enable-panning": true,
    "image-viewer-enable-filters": true,
    "pdf-viewer-auto-scale": true,
    "pdf-viewer-rendering-quality": "high",
    "pdf-renderer-enable": true,
    "pdf-renderer-initial-zoom": "200",
    "pdf-renderer-text-selection": true,
    "pdf-renderer-annotations": true,
    "theme-plugin": "texlyre-wide-theme",
    "theme-variant": "atom_light",
    "typst-auto-compile-on-open": false,
    "latex-engine": "pdftex",
    "latex-texlive-endpoint": "https://texlive.emaily.re",
    "latex-store-cache": true,
    "latex-store-working-directory": false,
    "file-sync-enable": true,
    "file-sync-auto-interval": 10,
    "file-sync-hold-timeout": 30,
    "file-sync-request-timeout": 60,
    "file-sync-conflict-resolution": "prefer-latest",
    "file-sync-server-url": "https://filepizza.emaily.re",
    "file-sync-notifications": true,
    "file-tree-filesystem-drag-drop": true,
    "file-tree-internal-drag-drop": true,
    "collab-signaling-servers": "wss://ywebrtc.emaily.re",
    "collab-awareness-timeout": 30,
    "collab-auto-reconnect": false
  },
  "properties": {
    "sidebar-width:global": 502,
    "latex-output-width:global": 700,
    "sidebar-collapsed:global": false,
    "latex-output-collapsed:global": true,
    "log-visualizer-height:global": 600,
    "log-visualizer-collapsed:global": false,
    "pdf-renderer-zoom:global": 1,
    "pdf-renderer-scroll-view:global": true
  },
  "secrets": {}
}
